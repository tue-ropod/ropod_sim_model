<?xml version="1.0"?>
<robot name="visual">


    <link name='base_link'>
      <pose>0 0 0 0 -0 0</pose>



      <collision name='base_link_collision'>
        <pose>0 0 0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.6 0.6 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name='base_link_visual'>
        <pose>0 0 0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.6 0.6 0.1</size>
          </box>
        </geometry>
      </visual>

      <collision name='base_link_collision_left_rear_wheel'>
        <pose>-0.25 0.25 0.05 0 -0 0</pose>
        <geometry>
          <sphere>
            <radius>0.05</radius>
          </sphere>
        </geometry>
         <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
        <surface>
          <contact>
            <ode>
              <kp>1e+09</kp>
              <kd>1</kd>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>5</mu>
              <mu2>5</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='base_link_visual_left_rear_wheel'>
         <pose>-0.25 0.25 0.05 0 -0 0</pose>
        <geometry>
          <sphere>
            <radius>0.05</radius>
          </sphere>
        </geometry>
      </visual>


